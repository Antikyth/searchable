{
	"schema_version": 1,
	"quilt_loader": {
		"group": "io.github.antikyth",
		"id": "searchable",
		"version": "${version}",
		"metadata": {
			"name": "Searchable",
			"description": "Improves singleplayer world searching and adds search boxes for servers, languages, and keybinds.",
			"license": "MPL-2.0",
			"contributors": {
				"Antikyth": "Owner"
			},
			"contact": {
				"homepage": "https://github.com/Antikyth/searchable",
				"issues": "https://github.com/Antikyth/searchable/issues",
				"sources": "https://github.com/Antikyth/searchable"
			},
			"icon": "assets/searchable/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"pre_launch": "io.github.antikyth.searchable.Bootstrap",
			"client_init": "io.github.antikyth.searchable.Searchable"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.19.1"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=7.0.2"
			},
			{
				"id": "minecraft",
				"versions": ">=1.20"
			}
		]
	},
	"mixin": "searchable.mixins.json",
	"access_widener": "searchable.accesswidener",
	"minecraft": {
		"environment": "client"
	}
}
